<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".activity.register.GenderFragment">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:id="@+id/h_line"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <ImageButton
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:src="@mipmap/add_device"
        android:contentDescription="@string/sms_regain_code"
        app:layout_constraintBottom_toTopOf="@id/h_line"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="20dp"
        android:background="@null"
        />
    <ImageButton
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:src="@mipmap/add_device"
        android:contentDescription="@string/sms_regain_code"
        app:layout_constraintTop_toBottomOf="@id/h_line"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="20dp"
        android:background="@null"
        />

</android.support.constraint.ConstraintLayout>